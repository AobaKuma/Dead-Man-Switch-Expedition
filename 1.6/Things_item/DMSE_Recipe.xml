  <!-- 燒結爐 -->
 <RecipeDef>
    <defName>DMS_Make_TantastenAlloy</defName>
    <label>Make TantastenAlloy X20</label>
    <description>Sinter a small amount of tantastenAlloy.</description>
    <jobString>Sintering.</jobString>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>ConstructMetal</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <recipeUsers>
      <li>DMS_Furnace</li>
	    <li>DMS_LabFurnace</li>
    </recipeUsers>
    <skillRequirements>
      <Crafting>3</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <researchPrerequisite>DMSE_VacuumMaterial</researchPrerequisite>
    <workAmount>15000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Tungsteel</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>BlocksVacstone</li>
          </categories>
        </filter>
        <count>120</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Tungsteel</li>
        <li>BlocksVacstone</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <DMSE_TantastenAlloy>20</DMSE_TantastenAlloy>
    </products>
  </RecipeDef>

