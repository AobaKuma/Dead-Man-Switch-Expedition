<?xml version="1.0" encoding="utf-8"?>

<Defs>

  <!-- 低重力生產設施 -->
  <RecipeDef>
    <defName>DMS_Make_Graphene_Chemfuel</defName>
    <label>Make Graphene x5</label>
    <description>Producing a small amount of graphene materials using Chemfuel and wood.</description>
    <jobString>Producing.</jobString>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <recipeUsers>
      <li>DMSE_MicrogravityFacility</li>
    </recipeUsers>
    <skillRequirements>
      <Crafting>8</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <researchPrerequisite>DMSE_Microgravity</researchPrerequisite>
    <workAmount>15000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Chemfuel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <DMSE_Graphene>5</DMSE_Graphene>
    </products>
  </RecipeDef>

