<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="BuildingBase">
    <defName>DMSE_GrapheneBioreactor</defName>
    <label>Graphene bio-reactor</label>
    <!-- 一種固定式碳電生物反應器，能夠以二氧化碳等為碳源借助電力與恆溫環境合成作為生物營養來源的各類碳水化合物，需消耗石墨烯作為電極與催化載體提升反應效率，其人工合成澱粉與葡萄糖的能量效率約為農業種植的八倍，面積效率則高達三千倍。 -->
    <description>A stationary carbon-electric bioreactor. It uses carbon dioxide to synthesize various carbohydrates, a biological nutrient source. Requires graphene as a catalytic carrier during the reaction, operating in an environment of constant temperature and electricity to improve reaction efficiency.</description>
    <graphicData>
      <texPath>Things/Building/GrapheneBioreactor/building</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(5,5)</drawSize>
      <drawOffset>(0,0,0.105)</drawOffset>
      <damageData>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
    <staticSunShadowHeight>0.9</staticSunShadowHeight>
    <fillPercent>0.9</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <pathCost>42</pathCost>
    <tickerType>Normal</tickerType>
    <statBases>
      <MaxHitPoints>750</MaxHitPoints>
      <WorkToBuild>40000</WorkToBuild>
      <Mass>300</Mass>
      <Flammability>0.25</Flammability>
    </statBases>
    <size>(2,2)</size>
    <costList>
      <Steel>200</Steel>
      <Tungsteel>125</Tungsteel>
      <DMSE_Graphene>75</DMSE_Graphene>
      <ComponentStamped>10</ComponentStamped>
      <ComponentSpacer>1</ComponentSpacer>
    </costList>
    <building>
      <destroySound>BuildingDestroyed_Metal_Small</destroySound>
    </building>
    <researchPrerequisites>
      <li>DMSE_OrbitalDynamics</li>
    </researchPrerequisites>
    <designationCategory>DMS_Category</designationCategory>
    <minifiedDef>MinifiedThing</minifiedDef>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Flickable" />
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>500</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
        <!-- <soundAmbientProducingPower>ScannerLongRangeMineral_Ambience</soundAmbientProducingPower> -->
      </li>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_BandNode</li>
    </placeWorkers>
  </ThingDef>
</Defs>